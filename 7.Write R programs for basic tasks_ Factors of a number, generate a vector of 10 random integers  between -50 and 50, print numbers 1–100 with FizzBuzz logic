
num <- 36
factors <- c()
for (i in 1:num) {
  if (num %% i == 0) {
    factors <- c(factors, i)
  }
}
cat("Factors of", num, "are:", factors, "\n\n")

set.seed(123)  # reproducibility
rand_vec <- sample(-50:50, 10, replace = TRUE)
cat("Random Vector (10 integers between -50 and 50):\n")
print(rand_vec)
cat("\n")

cat("FizzBuzz from 1 to 100:\n")
for (i in 1:100) {
  if (i %% 15 == 0) {
    print("FizzBuzz")
  } else if (i %% 3 == 0) {
    print("Fizz")
  } else if (i %% 5 == 0) {
    print("Buzz")
  } else {
    print(i)
  }
}
