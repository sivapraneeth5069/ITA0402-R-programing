
plot(0, 0, type = "n",
     xlim = c(0, 10),
     ylim = c(0, 100),
     xlab = "X-axis",
     ylab = "Y-axis",
     main = "Empty Plot Example 1")


plot(0, 0, type = "n",
     xlim = c(-50, 50),
     ylim = c(-10, 10),
     xlab = "X-axis",
     ylab = "Y-axis",
     main = "Empty Plot Example 2")
